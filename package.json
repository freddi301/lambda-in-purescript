{
  "version": "0.1.1",
  "name": "lambda-in-purescript",
  "scripts": {
    "run": "npm run copy:syntax && pulp run",
    "build": "pulp build && npm run copy:syntax",
    "test": "npm run copy:syntax && pulp test",
    "test:watch": "pulp -w test",
    "repl": "npm run copy:syntax && pulp repl",
    "docs": "pulp docs",
    "precommit": "npm test",
    "postinstall": "bower install",
    "prepublish": "npm run build",
    "copy:syntax": "nearleyc src/Lambda/Parser/syntax.ne -o output/Lambda.Parser.Parser/grammar.js"
  },
  "dependencies": {
    "moo": "^0.4.3",
    "nearley": "^2.11.0",
    "purescript": "^0.11.7"
  },
  "files": [
    "output",
    "src",
    "bower.json"
  ],
  "devDependencies": {
    "bower": "^1.8.4",
    "husky": "^0.14.3",
    "pulp": "^12.0.1",
    "purescript": "^0.11.7"
  },
  "license": "MIT"
}
