    "copy:syntax": "cp -u src/Lambda/Parser/syntax.ne output/Lambda.Parser.Parser/syntax.ne"
  },
  "dependencies": {
    "nearley-make": "^1.0.1"
  },
  "files": ["output"],
  "devDependencies": {
    "bower": "^1.8.2",
    "husky": "^0.14.3",
    "pulp": "^12.0.1"
  },
  "license": "MIT"
}
